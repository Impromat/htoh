            </div>
          </div>
          <div>
            <div class="text-center serv_logos">
              <%= image_tag "Cook.svg", class: "img-responsive" %>
              <p>Cuisine</p>
            </div>
          </div>
          <div>
            <div class="text-center serv_logos">
              <%= image_tag "Movin.svg", class: "img-responsive" %>
              <p>Déménagement</p>
            </div>
          </div>
          <div>
            <div class="text-center serv_logos">
              <%= image_tag "School.svg", class: "img-responsive" %>
