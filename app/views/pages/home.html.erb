            </div>
          </div>
          <div>
            <div class="text-center serv_logos">
              <%= image_tag "Line.svg", class: "img-responsive" %>
              <p>Faire la queue</p>
            </div>
          </div>
          <div>
            <div class="text-center serv_logos">
              <%= image_tag "Car.svg", class: "img-responsive" %>
